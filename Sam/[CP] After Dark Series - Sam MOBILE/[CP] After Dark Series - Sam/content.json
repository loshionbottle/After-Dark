        },
        {
            "LogName": "After Dark Sam's Dialogue",
            "Action": "Include",
            "FromFile": "Data/Dialogue/Dialogue_Sam.json, Data/Dialogue/MarriageDialogue_Sam.json, Data/Dialogue/DialogueTriggers_Sam.json"
